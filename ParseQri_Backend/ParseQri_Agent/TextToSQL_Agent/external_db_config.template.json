{
  "external_database": {
    "enabled": true,
    "type": "mysql",
    "host": "YOUR_HOST",
    "port": 3306,
    "database": "YOUR_DATABASE_NAME",
    "user": "YOUR_USERNAME",
    "password": "YOUR_PASSWORD"
  },
  "user_mapping": {
    "1": "external_user",
    "default_user": "external_user"
  },
  "table_mapping": {
    "direct_access": true,
    "no_user_prefix": true
  }
} 