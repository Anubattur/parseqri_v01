      }
    },
    "schema_management": {
      "module": "agents.schema_management",
      "class": "SchemaManagementAgent",
      "params": {}
    },
    "advanced_visualization": {
      "module": "agents.advanced_visualization",
      "class": "AdvancedVisualizationAgent",
      "params": {}
