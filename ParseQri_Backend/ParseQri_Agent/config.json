      "params": {
        "llm_model": "qwen2.5",
        "api_base": "http://localhost:11434"
      }
    },
    "sql_validation": {
      "module": "agents.sql_validation",
      "class": "SQLValidationAgent",
      "params": {
        "llm_model": "orca2",
        "api_base": "http://localhost:11434"
      }
    },
    "query_execution": {
      "module": "agents.query_execution",
      "class": "QueryExecutionAgent",
      "params": {}
